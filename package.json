{
  "name": "issue-to-pr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.6.4",
  "scripts": {
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:up:prod": "docker compose -f docker/docker-compose.yml --profile prod up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "predev": "pnpm docker:up && pnpm i",
    "dev": "concurrently --names \"next,worker,smee\" --prefix-colors \"cyan,yellow,magenta\" \"next dev\" \"pnpm dev:workflow-workers\" \"pnpm smee\"",
    "dev:next": "next dev",
    "dev:workflow-workers": "pnpm --filter workflow-workers dev",
    "prestart": "pnpm docker:up:prod",
    "start": "next start",
    "start:workflow-workers": "pnpm --filter workflow-workers start",
    "build": "next build",
    "build:workflow-workers": "pnpm --filter workflow-workers build",
    "build:shared": "pnpm --filter shared build",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:tests": "tsc --noEmit -p __tests__/tsconfig.json",
    "check:all": "next lint; prettier --check .; tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:components": "jest --selectProjects dom",
    "test:node": "jest --selectProjects node",
    "test:agent": "NODE_OPTIONS='--experimental-vm-modules' jest -c __tests__/config/jest.config.agent.ts",
    "test:neo4j": "jest -c __tests__/config/jest.config.neo4j.ts",
    "test:fs": "jest \"**/*.integration.test.ts\"",
    "test:infra:up": "docker compose -f docker/docker-compose.e2e-test.yml up -d --wait",
    "test:infra:down": "docker compose -f docker/docker-compose.e2e-test.yml down -v",
    "test:e2e": "jest -c __tests__/config/jest.config.e2e.ts",
    "test:e2e:run": "pnpm test:infra:up && pnpm test:e2e; pnpm test:infra:down",
    "storybook": "pnpm --filter @issue-to-pr/storybook dev",
    "build-storybook": "pnpm --filter @issue-to-pr/storybook build",
    "smee": "dotenv -e .env.local -- sh -c 'smee -u $SMEE_URL --path /api/webhook/github --port 3000'"
  },
  "dependencies": {
    "@hookform/resolvers": "4",
    "@next/bundle-analyzer": "^15.3.3",
    "@octokit/auth-app": "^7.2.0",
    "@octokit/auth-oauth-user": "^6.0.0",
    "@octokit/graphql": "^8.2.1",
    "@octokit/openapi-types": "^24.0.0",
    "@octokit/rest": "^21.0.2",
    "@octokit/webhooks": "^14.2.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/dockerode": "^3.3.42",
    "@upstash/redis": "^1.34.4",
    "@vercel/otel": "^1.13.0",
    "bullmq": "~5.56.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dockerode": "^4.0.7",
    "exponential-backoff": "^3.1.2",
    "framer-motion": "^12.4.10",
    "gray-matter": "^4.0.3",
    "ioredis": "^5.3.4",
    "langfuse": "^3.32.0",
    "langfuse-core": "^3.37.0",
    "lucide-react": "^0.460.0",
    "motion": "^11.18.0",
    "neo4j-driver": "^5.28.1",
    "next": "^14.2.35",
    "next-auth": "^5.0.0-beta.25",
    "octokit": "^4.0.2",
    "openai": "^5.12.2",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "7",
    "react-markdown": "^10.1.0",
    "redis": "^4.7.0",
    "simple-git": "^3.27.0",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@octokit/types": "^15.0.1",
    "@octokit/webhooks-types": "^7.6.1",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/nextjs-vite": "^9.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint": "^0.40.0",
    "mini-css-extract-plugin": "^2.9.2",
    "msw": "^2.7.3",
    "playwright": "^1.55.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "smee-client": "^5.0.0",
    "storybook": "^9.1.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.19.0",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "jest": {
    "projects": [
      "<rootDir>/__tests__/config/jest.config.dom.ts",
      "<rootDir>/__tests__/config/jest.config.node.ts"
    ],
    "rootDir": "."
  }
}
