{
  "name": "shared",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && tsc-alias && tsc-esm-fix",
    "start": "node dist/index.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@octokit/graphql": "^8.2.1",
    "@octokit/rest": "^21.1.0",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2",
    "openai": "^5.12.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsc-alias": "^1.8.16",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5"
  },
  "peerDependencies": {
    "zod": ">=3.24.0"
  }
}
