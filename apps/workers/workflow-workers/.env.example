REDIS_URL=
OPENAI_API_KEY=
NEO4J_URI=
NEO4J_USER=
NEO4J_PASSWORD=
GITHUB_APP_ID=
GITHUB_APP_PRIVATE_KEY_PATH=
SHUTDOWN_TIMEOUT_MS=3600000

# Optional: URL of the web app where workflow runs can be viewed
# Production example: WEB_APP_URL=https://issuetopr.com
# If not set, workflow references will show ID only
WEB_APP_URL=

# Optional: Human-readable name for this environment (e.g., "2018 MacBook Pro", "Production Server")
# Shown in workflow references when WEB_APP_URL is not set
ENVIRONMENT_NAME=

# =============================================================================
# OBSERVABILITY - LANGFUSE
# =============================================================================
# Langfuse provides LLM observability, tracing, and analytics.
# Create an account at: https://langfuse.com/

# [Optional] Langfuse public key
LANGFUSE_PUBLIC_KEY=

# [Optional] Langfuse secret key
LANGFUSE_SECRET_KEY=

# [Optional] Langfuse API base URL
# Default: https://cloud.langfuse.com (EU) or https://us.cloud.langfuse.com (US)
LANGFUSE_BASEURL=https://us.cloud.langfuse.com